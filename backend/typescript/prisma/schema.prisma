generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model test {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model users {
  id             String          @id @default(auto()) @map("_id") @db.ObjectId
  v              Int             @map("__v")
  authId         String
  email          String
  firstName      String
  lastName       String
  role           String
}

//tina-emma/kitchen-request-model
model kitchenRequest {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  v             Int      @map("__v")
  requestName   String
  requester     User     @relation(fields: [requesterId], references: [id])
  requesterId   String     
  location      String
  shiftTime     DateTime
  meal          String
  cookingMethod String
  frequency     String
}