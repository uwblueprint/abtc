generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model test {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model users {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  v         Int    @map("__v")
  authId    String
  email     String
  firstName String
  lastName  String
  role      String
}

model users_signed_up {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  email     String
  firstName String
  lastName  String
  adminID   String
}
