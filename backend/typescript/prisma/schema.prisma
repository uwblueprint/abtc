generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model test {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model users {
   id        String @id @default(auto()) @map("_id") @db.ObjectId
   authId    String
   email     String
   firstName String
   lastName  String
   role      Role
 }


enum Role {
  VOLUNTEER
  ADMIN
}
